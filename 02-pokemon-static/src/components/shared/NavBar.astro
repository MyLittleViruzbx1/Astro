---

const links = [
    {href: '/', text: 'Listado'},
    {href: '/puchamones/1', text: 'Pagina'},
    {href: '/favorites', text: 'Favoritos'},
]

---

<!-- component -->
<nav class="shadow bg-gray-800">
    <div class="container flex items-center justify-center p-6 mx-auto text-gray-600 capitalize dark:text-gray-300">

        {
            links.map( ({href, text}) =>(
                <a href={href} class="text-gray-800 dark:text-gray-200 border-b-2 border-blue-500 mx-1.5 sm:mx-6">{text}</a>

            ))
        }





       
    </div>
</nav>