---
import type { boolean, number, string } from "astro:schema";
import Navbar from "../pages/components/Navbar.astro";

import "../style/global.css";

interface Props {
  title: string;
  Age?: number;
  isActive?: boolean;
}

const { title, Age, isActive } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
</html>
<body>
  <Navbar />
  <slot />
</body>
